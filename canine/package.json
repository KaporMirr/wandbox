{
  "private": true,
  "name": "canine",
  "description": "",
  "license": "",
  "scripts": {
    "build": "remix build",
    "build:css": "sass --load-path=node_modules styles/:app/styles/",
    "dev": "remix watch",
    "dev:css": "sass --load-path=node_modules styles/:app/styles/ --watch ",
    "postinstall": "remix setup cloudflare-workers",
    "build:worker": "esbuild --define:process.env.NODE_ENV='\"production\"' --minify --bundle --sourcemap --outdir=dist ./worker",
    "dev:worker": "esbuild --define:process.env.NODE_ENV='\"development\"' --bundle --sourcemap --outdir=dist ./worker",
    "start": "miniflare --build-command \"npm run dev:worker\" --binding WANDBOX_URL_PREFIX=\"https://api-develop.wandbox.org\" --binding WANDBOX_GITHUB_CLIENT_ID=50835a4ce149db7aeedc --binding WANDBOX_GITHUB_CLIENT_SECRET=9d7a83fcb5f0ff7cd0c209206e1b9e1b4dcecd3e --binding WANDBOX_KV_COOKIE_SECRET=12345678 --watch",
    "deploy:master": "rm -rf public/build && npm run build && wrangler publish --env master",
    "deploy:develop": "rm -rf public/build && npm run build && wrangler publish --env develop",
    "deploy:develop:tail": "rm -rf public/build && npm run build && wrangler publish --env develop && wrangler tail --env develop --format pretty"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^0.19.13",
